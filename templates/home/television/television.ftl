<div class="row television">
  <div class="head">
    <img
      class="img-responsive" 
      src="/activos/assets/img/iguana-television.jpg" 
      alt="La Iguana Television">
  </div>
  <div class="owl-carousel-television">
    <#list [1,2,3,4,5] as index>
      <div class="destacado-item">
        <img src="http://placehold.it/560x315" class="img-responsive" alt="">
      </div>
    </#list>
  </div>
  <div class="list-videos">
    <div class="row">
      <#list [1,2,3,4,5,6] as index>
        <div class="video-item col-md-2">
          <img src="http://placehold.it/560x315" class="img-responsive" alt="">      
        </div>
      </#list>
    </div>
  </div>

</div>


